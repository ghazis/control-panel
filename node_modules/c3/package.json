    "codecov": "^2.2.0",
    "jasmine-core": "^2.3.4",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.1.0",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-spec-reporter": "0.0.31",
    "node-sass": "^4.5.3",
    "node-static": "^0.7.9",
    "rollup": "^0.41.6",
